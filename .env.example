# Chrome Extension Environment Variables
# Copy this file to .env.local and fill in your values

# Development
NODE_ENV=development
PLASMO_PUBLIC_EXTENSION_NAME="FluentFlow"
PLASMO_PUBLIC_VERSION="1.0.0"

# API Configuration
PLASMO_PUBLIC_API_BASE_URL=https://api.example.com
PLASMO_PUBLIC_API_TIMEOUT=30000

# AI Services
PLASMO_PUBLIC_OPENAI_API_KEY=sk-your-openai-key-here
PLASMO_PUBLIC_ANTHROPIC_API_KEY=sk-ant-your-anthropic-key-here
PLASMO_PUBLIC_AI_DEFAULT_PROVIDER=openai

# Payment & Monetization
PLASMO_PUBLIC_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-key-here
STRIPE_SECRET_KEY=sk_test_your-stripe-secret-key-here
PLASMO_PUBLIC_PAYMENT_API_URL=https://api.yourbackend.com

# Analytics & Monitoring
PLASMO_PUBLIC_ANALYTICS_ID=your-analytics-id
PLASMO_PUBLIC_SENTRY_DSN=https://your-sentry-dsn
PLASMO_PUBLIC_ENABLE_ANALYTICS=true

# Feature Flags
PLASMO_PUBLIC_ENABLE_AI_FEATURES=true
PLASMO_PUBLIC_ENABLE_PAYMENT_FEATURES=true
PLASMO_PUBLIC_ENABLE_ANALYTICS=true
PLASMO_PUBLIC_ENABLE_DEBUG_MODE=false

# Security
JWT_SECRET=your-jwt-secret-key-here
ENCRYPTION_KEY=your-encryption-key-here
PLASMO_PUBLIC_CSP_NONCE=your-csp-nonce

# Development Tools
PLASMO_PUBLIC_ENABLE_DEVTOOLS=true
PLASMO_PUBLIC_LOG_LEVEL=debug
PLASMO_PUBLIC_MOCK_API=false

# Extension Store
PLASMO_PUBLIC_CHROME_WEB_STORE_ID=your-extension-id
PLASMO_PUBLIC_FIREFOX_ADDON_ID=your-addon-id

