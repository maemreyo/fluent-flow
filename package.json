{
  "name": "fluent-flow",
  "displayName": "FluentFlow - Master Interactive Video Lessons",
  "version": "1.0.0",
  "description": "Enhance language learning on YouTube with interactive video looping, AI-generated comprehension questions, and integrated learning analytics.",
  "author": "Matthew Ngo",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build",
    "package": "plasmo package",
    "typecheck": "tsc --noEmit",
    "lint": "eslint . --ext ts,tsx",
    "lint:fix": "eslint . --ext ts,tsx --fix",
    "format": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "setup": "node scripts/setup.js",
    "configure": "node scripts/configure.js",
    "dev-menu": "node scripts/dev-workflow.js",
    "deploy": "node scripts/deploy.js",
    "security:audit": "npm audit --audit-level moderate",
    "security:fix": "npm audit fix",
    "analyze": "npm run build && npx webpack-bundle-analyzer build/static/js/*.js",
    "clean": "rm -rf build dist .plasmo node_modules/.cache",
    "reinstall": "npm run clean && npm install",
    "validate": "npm run typecheck && npm run lint && npm run test:ci",
    "prepare": "husky install || true",
    "release": "semantic-release"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.20.9",
    "@hookform/resolvers": "^5.2.1",
    "@plasmohq/storage": "^1.9.0",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-aspect-ratio": "^1.1.7",
    "@radix-ui/react-avatar": "^1.1.10",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-context-menu": "^2.2.16",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-hover-card": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-navigation-menu": "^1.2.14",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-toggle": "^1.1.10",
    "@radix-ui/react-toggle-group": "^1.1.11",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@supabase/supabase-js": "^2.55.0",
    "@tanstack/react-query": "^5.85.5",
    "@tanstack/react-query-devtools": "^5.85.5",
    "ai": "^3.1.12",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "cmdk": "^1.1.1",
    "date-fns": "^3.6.0",
    "embla-carousel-react": "^8.6.0",
    "input-otp": "^1.4.2",
    "jsonrepair": "^3.13.0",
    "jwt-decode": "^4.0.0",
    "ky": "^1.2.3",
    "lucide-react": "^0.535.0",
    "marked": "^12.0.2",
    "next-themes": "^0.4.6",
    "openai": "^4.47.1",
    "plasmo": "^0.88.0",
    "prismjs": "^1.29.0",
    "react": "18.2.0",
    "react-day-picker": "^9.9.0",
    "react-dom": "18.2.0",
    "react-h5-audio-player": "^3.10.0",
    "react-hook-form": "^7.62.0",
    "react-resizable-panels": "^3.0.4",
    "recharts": "2.15.4",
    "sharp": "^0.34.3",
    "sonner": "^2.0.7",
    "stripe": "^14.25.0",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "uuid": "^9.0.1",
    "vaul": "^1.1.2",
    "zod": "^3.25.76",
    "zustand": "^4.5.2"
  },
  "devDependencies": {
    "@ianvs/prettier-plugin-sort-imports": "4.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.16",
    "@testing-library/jest-dom": "^6.6.4",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/chrome": "0.0.258",
    "@types/jest": "^30.0.0",
    "@types/node": "20.11.5",
    "@types/react": "18.2.48",
    "@types/react-dom": "18.2.18",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^30.0.5",
    "prettier": "3.2.4",
    "prettier-plugin-tailwindcss": "0.6.11",
    "tailwindcss": "3.4.1",
    "ts-jest": "^29.4.1",
    "typescript": "5.3.3"
  },
  "manifest": {
    "host_permissions": [
      "https://www.youtube.com/*",
      "https://youtube.com/*",
      "https://generativelanguage.googleapis.com/*",
      "http://localhost:3838/*"
    ],
    "permissions": [
      "storage",
      "activeTab",
      "contextMenus",
      "sidePanel",
      "tabs",
      "tabCapture"
    ],
    "commands": {
      "toggle-loop": {
        "suggested_key": {
          "default": "Alt+L"
        },
        "description": "Set A/B loop points"
      },
      "toggle-recording": {
        "suggested_key": {
          "default": "Alt+R"
        },
        "description": "Start/stop voice recording"
      },
      "compare-audio": {
        "suggested_key": {
          "default": "Alt+C"
        },
        "description": "Compare original vs recorded audio"
      },
      "toggle-panel": {
        "suggested_key": {
          "default": "Alt+F"
        },
        "description": "Toggle FluentFlow panel"
      }
    },
    "side_panel": {
      "default_path": "sidepanel.html"
    },
    "web_accessible_resources": [
      {
        "resources": [
          "assets/*"
        ],
        "matches": [
          "https://www.youtube.com/*",
          "https://youtube.com/*"
        ]
      }
    ]
  }
}
